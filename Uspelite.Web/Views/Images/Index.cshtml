@model dynamic

@{
    ViewBag.Title = "title";
}

@using (Ajax.BeginForm("SaveImage", "Images", new AjaxOptions
{
    HttpMethod = "POST",
}, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    @Html.TextBox("imageFiles.CategoryName")
    <input type="file" name="model" id="third-image" multiple="multiple"/>
    <input type="submit" />
}